  "tray": {
    "icon-size": 21,
    "spacing": 6,
  },
  "clock": {
    // "format": "{:%A  ï‰²  %B-%d-%Y  ïº  %I:%M:%S %p}",
    "format": "<span size=\"15000\">{:%H:%M}</span>",
