{
  "layer": "top",
  "position": "top",
  "mode": "dock",
  "margin-top": 15,
  "margin-left": 15,
  "margin-right": 15,
  "exclusive": true,
  "passthrough": false,
  "gtk-layer-shell": true,
  "reload_style_on_change": true,
  "height": 0,
  "modules-left": [
    "custom/rofi",
    "custom/space",
    // "custom/exchange-rate",
    "hyprland/workspaces",
  ],
  "modules-center": [
    "hyprland/window"
  ],
  "modules-right": [
    "tray",
    "custom/space",
    "battery",
    "custom/sunset",
    "custom/space",
    "backlight",
    "pulseaudio",
    "custom/space",
    "clock",
  ],
  "hyprland/window": {
    "format": "{}",
    "separate-outputs": true,
  },
  "hyprland/workspaces": {
    "format": "<span size=\"15000\">{icon}</span>",
    "format-icons": {
