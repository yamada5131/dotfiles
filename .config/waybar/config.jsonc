  "custom/exchange-rate": {
    "exec": "go run $HOME/.config/scripts/exchange-rate/main.go",
    "format": "ï…— {}",
    "interval": 3600,
  },
  "custom/sunset": {
    "interval": "once",
    "tooltip": true,
    "return-type": "json",
    "format": "{icon}",
    "format-icons": {
      "on": "ïƒ« on",
      "off": "ïƒ« off",
