  "memory": {
    "states": {
      "c": 90, // critical
      "h": 60, // high
      "m": 30, // medium
    },
    "interval": 10,
    "format": "ó°¾† {used}GB",
    "format-m": "ó°¾… {used}GB",
    "format-h": "ó°“… {used}GB",
    "format-c": "ï¡ {used}GB",
    "format-alt": "ó°¾† {percentage}%",
